class Solution {
    public int[] twoSum(int[] numberList, int targetNumber) {
        for (int i = 0; i < numberList.length; i++) {
            for (int j = (i + 1); j < numberList.length; j++) {
                if (numberList[i] + numberList[j] == targetNumber) {
                    return new int[] {i, j};
                }
            }
        }
    return new int[] {};
    }
}
